{
  "headerAuth": {
    "body": "-H \"Authorization: Bearer $gbjwt$1\" \\\n$2",
    "description": "Authorization Header",
    "prefix": "hauth"
  },
  "contenTypeJson": {
    "body": "-H 'content-type: application/json' \\\n$1",
    "description": "contenTypeJson Header",
    "prefix": "hjson"
  },
  "contenTypeForm": {
    "body": "-H 'content-type: multipart/form-data' \\\n$1",
    "description": "contenTypeJson Header",
    "prefix": "hform"
  },
  "url": {
    "body": "--url \"http://localhost:$1\" \\\n$2",
    "description": "url",
    "prefix": "url"
  },
  "curl": {
    "body": "curl --url \"http://localhost:7702$1\" --request POST \\\n    --data '{\n    \"$2\"\n  }' \\\n  -H 'content-type: application/json' \\\n  -H \"Authorization: Bearer \\$gbjwt\"",
    "description": "curl",
    "prefix": "curl"
  }
}
