{
  "headerAuth": {
    "body": "-H \"Authorization: Bearer $1\" \\\n$2",
    "description": "Authorization Header",
    "prefix": "hauth"
  },
  "contenTypeJson": {
    "body": "-H 'content-type: application/json' \\\n$1",
    "description": "contenTypeJson Header",
    "prefix": "hjson"
  },
  "contenTypeForm": {
    "body": "-H 'content-type: multipart/form-data' \\\n$1",
    "description": "contenTypeJson Header",
    "prefix": "hform"
  },
  "url": {
    "body": "--url \"http://localhost:$1\" \\\n$2",
    "description": "url",
    "prefix": "url"
  },
  "curl": {
    "body": "curl --request POST \\\n  -H 'content-type: application/json' \\\n  --data '{\n    \"$1\"\n  }' \\\n  --url \"http://localhost:3000$2\" \n",
    "description": "curl",
    "prefix": "curl"
  }
}
